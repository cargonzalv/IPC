  "version": "4.8.0",
  "type": "module",
  "engines": {
    "node": "18.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "node builder-check.cjs",
    "build-stg": "NODE_ENV=staging node builder-check.cjs",
    "build-prd": "NODE_ENV=prod node builder-check.cjs",
    "build-component": "vite build --config component.vite.config.ts",
    "build-component:staging": "NODE_ENV=staging vite build --config component.vite.config.ts --mode staging",
    "build-component:prod": "vite build --config component.vite.config.ts --mode production",
    "build-playground": "vite build --config playground.vite.config.ts",
    "build-playground:staging": "NODE_ENV=staging vite build --config playground.vite.config.ts --mode staging",
    "build-playground:prod": "vite build --config playground.vite.config.ts --mode production",
    "build-wrapper": "vite build--config femsa-wrapper.vite.config.ts",
    "build-wrapper:staging": "NODE_ENV=staging vite build --config femsa-wrapper.vite.config.ts --mode staging",
    "build-wrapper:prod": "vite build --config femsa-wrapper.vite.config.ts --mode production",
    "build-wrapped-component": "vite build --config wrapped-component.vite.config.ts",
    "build-wrapped-component:staging": "NODE_ENV=staging vite build --config wrapped-component.vite.config.ts --mode staging",
    "build-wrapped-component:prod": "vite build --config wrapped-component.vite.config.ts --mode production",
    "eslint-check": "eslint --ext .js,.jsx,.ts,.tsx src",
    "preview": "vite preview",
    "test:watch": "jest --watch",
    "test": "jest",
    "test:c": "jest --coverage --runInBand --debug",
    "test:ci": "jest --config=jest.config.ci.cjs  --coverage --runInBand"
  },
